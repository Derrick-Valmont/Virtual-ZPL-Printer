{
	//
	// Aliases are global and can be used anywhere.
	//
	"aliases": [
		{
			"Key": "IHostedService",
			"TypeDefinition": "Microsoft.Extensions.Hosting.IHostedService, Microsoft.Extensions.Hosting.Abstractions"
		}
	],
	"services": [
		{
			"ServiceType": "Prism.Events.IEventAggregator, Prism",
			"ImplementationType": "Prism.Events.EventAggregator, Prism",
			"Lifetime": "Singleton"
		},
		{
			//
			// IMainWindow is used for the startup window.
			//
			"ServiceType": "Diamond.Core.Wpf.IMainWindow, Diamond.Core.Wpf.Abstractions",
			"ImplementationType": "VirtualZplPrinter.Views.MainView, VirtualZplPrinter",
			"Lifetime": "Singleton"
		},
		{
			"ServiceType": "VirtualZplPrinter.ViewModels.MainViewModel, VirtualZplPrinter",
			"ImplementationType": "VirtualZplPrinter.ViewModels.MainViewModel, VirtualZplPrinter",
			"Lifetime": "Singleton"
		},
		{
			"ServiceType": "VirtualZplPrinter.Client.TcpListenerClientHandler, VirtualZplPrinter",
			"ImplementationType": "VirtualZplPrinter.Client.TcpListenerClientHandler, VirtualZplPrinter",
			"Lifetime": "Transient"
		},
		{
			"ServiceType": "<IHostedService>",
			"ImplementationType": "VirtualZplPrinter.HostedServices.TcpListenerService, VirtualZplPrinter",
			"Lifetime": "Singleton"
		}
	]
}