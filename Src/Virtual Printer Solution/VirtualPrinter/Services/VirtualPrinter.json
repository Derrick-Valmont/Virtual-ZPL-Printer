{
	//
	// Aliases are global and can be used anywhere.
	//
	"aliases": [
		{
			"Key": "IHostedService",
			"TypeDefinition": "Microsoft.Extensions.Hosting.IHostedService, Microsoft.Extensions.Hosting.Abstractions"
		},
		{
			"Key": "Host",
			"TypeDefinition": "VirtualZplPrinter.Models.Host, VirtualZplPrinter"
		},
		{
			"Key": "LabelUnit",
			"TypeDefinition": "VirtualZplPrinter.Models.LabelUnit, VirtualZplPrinter"
		},
		{
			"Key": "Resolution",
			"TypeDefinition": "VirtualZplPrinter.Models.Resolution, VirtualZplPrinter"
		}
	],
	"services": [
		{
			"ServiceType": "Prism.Events.IEventAggregator, Prism",
			"ImplementationType": "Prism.Events.EventAggregator, Prism",
			"Lifetime": "Singleton"
		},
		{
			//
			// IMainWindow is used for the startup window.
			//
			"ServiceType": "Diamond.Core.Wpf.IMainWindow, Diamond.Core.Wpf.Abstractions",
			"ImplementationType": "VirtualZplPrinter.Views.MainView, VirtualZplPrinter",
			"Lifetime": "Singleton"
		},
		{
			"ServiceType": "VirtualZplPrinter.Views.ConfigurationView, VirtualZplPrinter",
			"ImplementationType": "VirtualZplPrinter.Views.ConfigurationView, VirtualZplPrinter",
			"Lifetime": "Transient"
		},
		{
			"ServiceType": "VirtualZplPrinter.Views.SendTestView, VirtualZplPrinter",
			"ImplementationType": "VirtualZplPrinter.Views.SendTestView, VirtualZplPrinter",
			"Lifetime": "Transient"
		},
		{
			"ServiceType": "VirtualZplPrinter.ViewModels.MainViewModel, VirtualZplPrinter",
			"ImplementationType": "VirtualZplPrinter.ViewModels.MainViewModel, VirtualZplPrinter",
			"Lifetime": "Singleton"
		},
		{
			"ServiceType": "VirtualZplPrinter.ViewModels.ConfigurationViewModel, VirtualZplPrinter",
			"ImplementationType": "VirtualZplPrinter.ViewModels.ConfigurationViewModel, VirtualZplPrinter",
			"Lifetime": "Transient"
		},
		{
			"ServiceType": "VirtualZplPrinter.ViewModels.SendTestViewModel, VirtualZplPrinter",
			"ImplementationType": "VirtualZplPrinter.ViewModels.SendTestViewModel, VirtualZplPrinter",
			"Lifetime": "Transient"
		},
		{
			"ServiceType": "VirtualZplPrinter.Client.TcpListenerClientHandler, VirtualZplPrinter",
			"ImplementationType": "VirtualZplPrinter.Client.TcpListenerClientHandler, VirtualZplPrinter",
			"Lifetime": "Transient"
		},
		{
			"ServiceType": "<IHostedService>",
			"ImplementationType": "VirtualZplPrinter.HostedServices.TcpListenerService, VirtualZplPrinter",
			"Lifetime": "Singleton"
		},
		//
		// Repositories
		//
		{
			"ServiceType": "Diamond.Core.Repository.IRepository`1[[<Host>]], Diamond.Core.Repository.Abstractions",
			"ImplementationType": "VirtualZplPrinter.Repositories.HostRepository, VirtualZplPrinter",
			"Lifetime": "Singleton"
		},
		{
			"ServiceType": "Diamond.Core.Repository.IRepository`1[[<LabelRotation>]], Diamond.Core.Repository.Abstractions",
			"ImplementationType": "VirtualZplPrinter.Repositories.LabelRotationRepository, VirtualZplPrinter",
			"Lifetime": "Singleton"
		},
		{
			"ServiceType": "Diamond.Core.Repository.IRepository`1[[<LabelUnit>]], Diamond.Core.Repository.Abstractions",
			"ImplementationType": "VirtualZplPrinter.Repositories.LabelUnitRepository, VirtualZplPrinter",
			"Lifetime": "Singleton"
		},
		{
			"ServiceType": "Diamond.Core.Repository.IRepository`1[[<Resolution>]], Diamond.Core.Repository.Abstractions",
			"ImplementationType": "VirtualZplPrinter.Repositories.ResolutionRepository, VirtualZplPrinter",
			"Lifetime": "Singleton"
		}
	]
}